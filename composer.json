{
    "name": "jerowork/aura-router-nested-middleware",
    "type": "library",
    "description": "PSR-15 middleware for Aura Router with nested-middleware route handling",
    "keywords": [
        "PSR-7",
        "PSR-15",
        "middleware",
        "router",
        "aura"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen de Graaf",
            "email": "info@jero.work",
            "homepage": "https://jero.work"
        }
    ],
    "require": {
        "php": "^7.0",
        "aura/router": "^3.1",
        "jerowork/middleware-request-handler": "^0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 || ^7.0",
        "zendframework/zend-diactoros": "^1.7 || ^2.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Jerowork\\AuraRouterNestedMiddleware\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jerowork\\AuraRouterNestedMiddleware\\Test\\": "tests/"
        }
    }
}
